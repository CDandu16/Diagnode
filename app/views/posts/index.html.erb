<h1 class = "text-center">Posts</h1>
<% @posts.reverse_each do |post| %>

  <div class="post">
    <h2><%= post.title %></h2>
    <p><%= post.module1 %></p>
    <p><%# button_to 'edit', edit_post_path(post)%></p>
    <td><%= button_to 'Show', post_path(post), :class => "btn btn-default", :method => :get %></td>
  </div>

<% end %>
<p><%= link_to "Add a new medical post", new_post_path %></p>
