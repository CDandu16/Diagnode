<h1 class = "text-center">Posts</h1>
<% @posts.each do |post| %>

  <div class="post">
    <h2><%= post.title %></h2>
    <p><%= post.module1%></p>
    <td><%= button_to 'Show', post_path(post), :class => "btn btn-default", :method => :get %></td>
  </div>

<% end %>
<div class = "text-center padding"><%= button_to 'Add a new medical post', new_post_path, :class => "btn btn-default", :method => :get %></div>
